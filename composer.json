{
    "name": "thoughtco/statamic-plausible-analytics",
    "description": "Get your Plausible Analytics data right from your Statamic dashboard.",
    "version": "1.2.1",
    "autoload": {
        "psr-4": {
            "Thoughtco\\Plausible\\": "src"
        }
    },
    "authors": [
        {
            "name": "Jack Whiting",
            "email": "hi@jackwhiting.co.uk",
            "homepage": "https://jackwhiting.co.uk",
            "role": "Developer"
        },
        {
            "name": "Thought Collective",
            "email": "hello@thoughtcollective.com",
            "homepage": "https://www.thoughtcollective.com",
            "role": "Developer"
        }
    ],
    "extra": {
        "statamic": {
            "name": "Plausible Analytics",
            "description": "Get your Plausible Analytics data right from your Statamic dashboard"
        },
        "laravel": {
            "providers": [
                "Thoughtco\\Plausible\\PlausibleServiceProvider"
            ]
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.2",
        "laravel/pint": "^1.17"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
